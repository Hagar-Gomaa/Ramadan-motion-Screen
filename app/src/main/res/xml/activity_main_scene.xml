<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
           <KeyAttribute
               motion:framePosition="20"
               android:scaleX="4"
               android:scaleY="4"/>
       </KeyFrameSet>
        <OnSwipe />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="408dp"
            android:layout_height="61dp"
            android:scaleX="1"
            motion:layout_constraintTop_toBottomOf="@id/lottieAnimationView"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/linearlayout"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.508"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_editor_absoluteY="570dp"
            motion:layout_editor_absoluteX="-35dp" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="397dp"
            android:layout_height="248dp"
            android:scaleX="1"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/lottieAnimationView2"
            motion:layout_constraintBottom_toTopOf="@+id/lottieAnimationView"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:layout_height="270dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="216dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="734dp"
            android:id="@+id/lottieAnimationView" />
        <Constraint
            motion:layout_editor_absoluteY="29dp"
            motion:layout_editor_absoluteX="227dp"
            android:id="@+id/ramadandate" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            motion:layout_constraintTop_toBottomOf="@id/lottieAnimationView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp"
            android:layout_marginTop="222dp" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="421dp"
            android:layout_height="145dp"
            android:scaleX="1.25"
            motion:layout_constraintTop_toBottomOf="@id/lottieAnimationView"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/linearlayout"
            motion:layout_editor_absoluteY="537dp"
            android:layout_marginTop="50dp" />
        <Constraint
            android:layout_height="352dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="383dp"
            android:scaleX="1.1"
            android:id="@+id/lottieAnimationView2"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:layout_height="438dp"
            android:layout_width="340dp"
            android:id="@+id/popup"
            android:visibility="visible"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="55dp"
            android:layout_marginLeft="28dp"
            android:layout_marginStart="28dp"
            motion:layout_editor_absoluteY="238dp" />
        <Constraint
            android:layout_height="344dp"
            android:layout_width="335dp"
            android:id="@+id/popup2"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="44dp"
            android:layout_marginRight="44dp"
            android:layout_marginTop="36dp"
            motion:layout_editor_absoluteX="32dp"
            motion:layout_editor_absoluteY="32dp" />
        <Constraint
            android:layout_height="251dp"
            android:layout_marginTop="220dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="432dp"
            android:id="@+id/lottieAnimationView"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="404dp"
            android:layout_height="78dp"
            motion:layout_editor_absoluteX="14dp"
            android:layout_width="140dp"
            android:id="@+id/heart" />
    </ConstraintSet>
</MotionScene>